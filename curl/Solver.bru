meta {
  name: Solver
  type: http
  seq: 1
}

post {
  url: http://localhost:8000/solve
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {"constraint": "(declare-const country String)\n(declare-const age Int)\n(declare-const gender String)\n(assert (and (= country \"vn\") (< age 20) (= gender \"male\")))\n(assert (and (= country \"vn\") (< age 16) (= gender \"male\")))"}
}

settings {
  encodeUrl: false
  timeout: 0
}
